{
  "name": "jest-helpers",
  "version": "1.0.0",
  "description": "A collection of helper functions to ease working with the Jest testing framework",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "prebuild": "npm run clean",
    "build": "tsc -p ./tsconfig.main.json",
    "commit": "commit",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codeandcats/jest-helpers.git"
  },
  "keywords": [
    "jest",
    "helper",
    "helpers",
    "describe",
    "mock",
    "partial"
  ],
  "author": "Ben <codeandcats@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codeandcats/jest-helpers/issues"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "homepage": "https://github.com/codeandcats/jest-helpers#readme",
  "devDependencies": {
    "@commitlint/cli": "^7.1.2",
    "@commitlint/config-conventional": "^7.1.2",
    "@commitlint/prompt-cli": "^7.1.2",
    "@types/node": "^10.11.4",
    "husky": "^1.1.0",
    "rimraf": "^2.6.2",
    "typescript": "^3.1.1"
  }
}